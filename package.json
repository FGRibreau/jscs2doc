{
  "name": "jscs2doc",
  "version": "0.0.8",
  "description": "Convert .jscsrc (javascript code-style configuration) to a human readable format",
  "bin": {
    "jscs2doc": "./bin/jscs2doc"
  },
  "keywords": [
    "jscs",
    "code-style",
    "documentation",
    "jscrsrc"
  ],
  "scripts": {
    "build": "babel src --plugins add-module-exports -d dist",
    "prepublish": "npm run build && git add dist/* && (git commit -m 'feat(dist)' || true)",
    "start": "./dist/bin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FGRibreau/jscs2doc.git"
  },
  "author": "Francois-Guillaume Ribreau <npm@fgribreau.com> (http://fgribreau.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FGRibreau/jscs2doc/issues"
  },
  "homepage": "https://github.com/FGRibreau/jscs2doc",
  "dependencies": {
    "babel": "^5.8.23",
    "highlight.js": "^8.7.0",
    "jscs": "^2.1.1",
    "lodash": "^3.10.1",
    "marked": "^0.3.5",
    "marked-toc": "^0.3.0",
    "when": "^3.7.3"
  },
  "devDependencies": {
    "babel-cli": "^6.6.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.2.0",
    "mocha": "^2.2.5"
  }
}
