{
  "name": "jscs2doc",
  "version": "0.0.10",
  "description": "Convert .jscsrc (javascript code-style configuration) to a human readable format",
  "bin": {
    "jscs2doc": "./bin/jscs2doc"
  },
  "scripts": {
    "build": "babel src --plugins add-module-exports -d dist",
    "release": "npm run build && git add dist/* && (git commit -m 'feat(dist)' || true)",
    "test":"cd examples/ && ../bin/jscs2doc",
    "start": "./bin/jscs2doc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FGRibreau/jscs2doc.git"
  },
  "keywords": [
    "jscs",
    "code-style",
    "documentation",
    "jscrsrc"
  ],
  "homepage": "https://github.com/FGRibreau/jscs2doc",
  "author": "Francois-Guillaume Ribreau <npm@fgribreau.com> (http://fgribreau.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FGRibreau/jscs2doc/issues"
  },
  "engines":{
    "node": "5"
  },
  "dependencies": {
    "highlight.js": "^8.7.0",
    "jscs": "^2.1.1",
    "lodash": "^3.10.1",
    "marked": "^0.3.5",
    "marked-toc": "^0.3.0",
    "when": "^3.7.3"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.7.5",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.6.0",
    "chai": "^3.2.0",
    "mocha": "^2.2.5"
  }
}
